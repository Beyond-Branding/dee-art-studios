---
import Layout from "../layout.astro";
---

<Layout>
  <Fragment slot="body">
    <div class="orb-content">
      <div class="orb-grid">
        <div class="orb-g-25 orb-sm-100">
          <div class="orb-position-fix">
            <div class="orb-photo-frame orb-obj-1">
              <img src="orabel/img/photo/36.jpg" alt="photo" />
            </div>
          </div>
        </div>
        <div class="orb-g-50 orb-sm-100">
          <div class="orb-main-title">
            <div class="orb-suptitle">
              <span class="orb-number">01</span>
              <span>Fashion Discover</span>
            </div>
            <h1>The wind <br />of change</h1>
            <div class="orb-button-frame">
              <div class="orb-btn-label">Show more photo for this project</div>
              <a href="portfolio-5.html" class="orb-sm-button">
                <div class="orb-arrow">
                  <img src="orabel/img/ui/arrow-1.svg" alt="arrow" />
                </div>
              </a>
            </div>
          </div>
        </div>
        <div class="orb-g-25 orb-sm-100">
          <div class="orb-object-frame">
            <div class="orb-photo-frame orb-obj-2">
              <img src="orabel/img/photo/37.jpg" alt="photo" />
            </div>
            <div class="orb-photo-frame orb-obj-3">
              <img src="orabel/img/photo/38.jpg" alt="photo" />
            </div>
          </div>
        </div>
      </div>
    </div>
  </Fragment>
</Layout>

    <link
      rel="icon"
      type="image/png"
      sizes="192x192"
      href="/kinsey/img/content/android-icon-192x192.png"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="32x32"
      href="/kinsey/img/content/favicon-32x32.png"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="96x96"
      href="/kinsey/img/content/favicon-96x96.png"
    />
    <link
